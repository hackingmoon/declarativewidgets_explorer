{
  "name": "declarativewidgets-explorer",
  "version": "0.2.0",
  "description": "Represents a widget that uses visualizations to explore an urth-core-dataframe",
  "main": "urth-viz-explorer.html",
  "authors": [
    "Jupyter Community"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/jupyter-incubator/declarativewidgets_explorer"
  },
  "keywords": [
    "urth",
    "visualization",
    "web-components"
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "polymer": "Polymer/polymer#^1",
    "iron-collapse": "PolymerElements/iron-collapse#^1",
    "iron-icon": "PolymerElements/iron-icon#^1",
    "iron-resizable-behavior": "PolymerElements/iron-resizable-behavior#^1",
    "paper-button": "PolymerElements/paper-button#^1",
    "paper-dialog": "PolymerElements/paper-dialog#^1",
    "paper-dropdown-menu": "PolymerElements/paper-dropdown-menu#^1",
    "paper-header-panel": "PolymerElements/paper-header-panel#^1",
    "paper-icon-button": "PolymerElements/paper-icon-button#^1",
    "paper-listbox": "PolymerElements/paper-listbox#^1",
    "paper-input": "PolymerElements/paper-input#^1",
    "paper-item": "PolymerElements/paper-item#^1",
    "paper-menu": "PolymerElements/paper-menu#^1",
    "paper-menu-button": "PolymerElements/paper-menu-button#^1",
    "paper-radio-group": "PolymerElements/paper-radio-group#^1",
    "paper-radio-button": "PolymerElements/paper-radio-button#^1",
    "paper-toolbar": "PolymerElements/paper-toolbar#^1",
    "vega-embed": "latest",
    "requirejs": "^2"
  },
  "devDependencies": {
    "web-component-tester": "*",
    "test-fixture": "PolymerElements/test-fixture",
    "iron-component-page": "PolymerElements/iron-component-page#^1"
  },
  "private": true
}
